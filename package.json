{
  "name": "fleet-master-pro",
  "type": "module",
  "version": "1.1.0",
  "description": "Fleet Management System with Multi-user support and Email Notifications",
  "main": "server.ts",
  "prisma": {
    "schema": "backend/prisma/schema.prisma"
  },
  "scripts": {
    "dev:server": "tsx --env-file backend/.env backend/src/server.ts",
    "dev:client": "vite",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "build": "vite build",
    "start": "tsx --env-file backend/.env backend/src/server.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.1.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^6.19.2",
    "@types/node-cron": "^3.0.11",
    "bcryptjs": "^2.4.3",
    "better-sqlite3": "^12.6.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^6.10.1",
    "pg": "^8.18.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.12.0",
    "recharts": "^3.7.0",
    "sql.js": "^1.13.0",
    "sweetalert2": "^11.26.18",
    "uuid": "^9.0.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.14",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-react": "^5.1.2",
    "concurrently": "^8.2.2",
    "prisma": "^6.19.2",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3",
    "vite": "^7.3.1"
  }
}
